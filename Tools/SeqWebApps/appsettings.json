{
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Title": "AI writing demo",
  "Description": "Given one or a few sentences, and AI will write the whole story for you.",
  "RepeatPenalty": 1,
  "Random": "",
  "Seq2Seq": {
    "CompilerOptions": "--use_fast_math",
    "GPUMemoryUsageRatio": 0.99,
    "DeviceIds": "0",
    "DistancePenalty": 2,
    "MaxSrcTokenSize": 2048,
    "MaxTgtTokenSize": 2048,
    "ModelFilePath": "..\\models\\writing\\ybook_base_v3.model",
    "ProcessorType": "CPU_MKL",
    "MKLInstructions": "AVX2",
    "RepeatPenalty": 5,
    "TokenGenerationStrategy": "Sampling",
    "TopPSampling": 0.0,
    "BeamSearchSize": 1,
    "BlockedTokens": "",
    "ModelType": "DecoderOnly",
    "LogVerbose": "Normal",
    "WordMappingFilePath": "",
    "EnableTensorCore": true
  },
  "SourceSpm": {
    "ModelFilePath": "..\\spm\\chsYBSpm.model"
  },
  "TargetSpm": {
    "ModelFilePath": "..\\spm\\chsYBSpm.model"
  }
}
